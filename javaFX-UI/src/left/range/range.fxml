<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="left.range.RangeController">
   <children>
      <Separator prefWidth="200.0" />
      <Button fx:id="addRangeButton" mnemonicParsing="false" onAction="#addRangeButtonOnAction" text="Add Range">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Button>
      <ComboBox fx:id="deleteRangeComboBox" onAction="#deleteRangeComboBoxOnAction" prefWidth="120.0" promptText="Delete Range">
         <VBox.margin>
            <Insets top="13.0" />
         </VBox.margin>
      </ComboBox>
      <ComboBox fx:id="displayRangeComboBox" onAction="#displayRangeComboBoxOnAction" prefWidth="125.0" promptText="Display Range">
         <VBox.margin>
            <Insets top="13.0" />
         </VBox.margin>
      </ComboBox>
   </children>
</VBox>
